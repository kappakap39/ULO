  </xsd:complexType>

  <xsd:complexType name="websiteM">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="website_code" type="xsd:string"/>
      <xsd:element minOccurs="0" name="website_reusult" type="xsd:string"/>
