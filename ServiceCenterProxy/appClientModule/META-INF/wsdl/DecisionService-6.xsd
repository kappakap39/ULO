  </xsd:complexType>

  <xsd:complexType name="kBankRsHeader">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="corrID" type="xsd:string"/>
      <xsd:element minOccurs="0" name="funcNm" type="xsd:string"/>
      <xsd:element minOccurs="0" name="rqUID" type="xsd:string"/>
