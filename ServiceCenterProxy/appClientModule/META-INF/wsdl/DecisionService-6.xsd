      <xsd:element minOccurs="0" name="userLangPref" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="response">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="return" type="tns:wfInquiryRespM"/>
    </xsd:sequence>
  </xsd:complexType>

