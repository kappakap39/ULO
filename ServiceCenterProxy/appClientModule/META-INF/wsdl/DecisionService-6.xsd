      <xsd:element maxOccurs="unbounded" minOccurs="0" name="websiteM" nillable="true" type="tns:websiteM"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="orCodeM">
    <xsd:sequence>
    	<xsd:element minOccurs="0" name="orCode" type="xsd:string"/>
