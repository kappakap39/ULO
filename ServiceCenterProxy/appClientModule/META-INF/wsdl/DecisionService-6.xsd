  		<xsd:element maxOccurs="1" minOccurs="0" name="country" type="xsd:string">
  		</xsd:element>
  		<xsd:element maxOccurs="1" minOccurs="0" name="subDistrict" type="xsd:string"/>
  	</xsd:sequence>
  </xsd:complexType>

