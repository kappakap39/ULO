  		<xsd:element maxOccurs="1" minOccurs="0" name="fico_error_msg" type="xsd:string">
  		</xsd:element>
  		<xsd:element maxOccurs="1" minOccurs="0" name="fico_reason1_code" type="xsd:string">
  		</xsd:element>
  		<xsd:element maxOccurs="1" minOccurs="0" name="fico_reason1_desc" type="xsd:string">
  		</xsd:element>
  		<xsd:element maxOccurs="1" minOccurs="0" name="fico_reason2_code" type="xsd:string">
  		</xsd:element>
  		<xsd:element maxOccurs="1" minOccurs="0" name="fico_reason2_desc" type="xsd:string">
  		</xsd:element>
  		<xsd:element maxOccurs="1" minOccurs="0" name="fico_reason3_code" type="xsd:string">
  		</xsd:element>
