	<parameter name="P_APP_DATE" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT DISTINCT TO_CHAR(APP.FINAL_APP_DECISION_DATE,'DD/MM/YYYY') AS FINAL_APP_DECISION_DATE,
        AG.APPLICATION_GROUP_NO,
        PER.MAIN_CARDLINK_CUST_NO,
        INC.TH_NAME,
        INC.CARDLINK_CUST_NO AS SUP_CARDLINK_CUST_NO,
        INC.CARD_NO_MARK,
        L.LOAN_AMT
FROM ORIG_APPLICATION_GROUP AG
JOIN ORIG_APPLICATION APP ON APP.APPLICATION_GROUP_ID = AG.APPLICATION_GROUP_ID AND APP.FINAL_APP_DECISION = 'Approve' AND APP.APPLICATION_TYPE = 'INC'
JOIN ORIG_APPLICATION_INCREASE INC ON AG.APPLICATION_GROUP_ID = INC.APPLICATION_GROUP_ID
INNER JOIN ORIG_LOAN L ON APP.APPLICATION_RECORD_ID = L.APPLICATION_RECORD_ID
LEFT JOIN ORIG_CARD C ON INC.MAIN_CARD_NO = C.CARD_NO
LEFT JOIN(
                SELECT DISTINCT CC2.CARDLINK_CUST_NO AS MAIN_CARDLINK_CUST_NO,A.APPLICATION_GROUP_ID
                FROM ORIG_PERSONAL_RELATION PR
                JOIN ORIG_PERSONAL_INFO P ON P.PERSONAL_ID = PR.PERSONAL_ID AND P.PERSONAL_TYPE = 'A'
                JOIN ORIG_APPLICATION A ON PR.REF_ID = A.APPLICATION_RECORD_ID
                JOIN BUSINESS_CLASS ON BUSINESS_CLASS.BUS_CLASS_ID = A.BUSINESS_CLASS_ID
                JOIN LIST_BOX_MASTER ON LIST_BOX_MASTER.FIELD_ID = '53' AND LIST_BOX_MASTER.CHOICE_NO = BUSINESS_CLASS.ORG_ID
                JOIN ORIG_CARDLINK_CUSTOMER CC2 ON P.PERSONAL_ID = CC2.PERSONAL_ID AND CC2.ORIG_ID = LIST_BOX_MASTER.MAPPING1
	WHERE A.BUSINESS_CLASS_ID NOT LIKE 'KEC%'
                )PER ON PER.APPLICATION_GROUP_ID = AG.APPLICATION_GROUP_ID
WHERE APP.CARDLINK_FLAG = 'S' AND AG.LAST_DECISION = 'AP'
AND APP.BUSINESS_CLASS_ID NOT LIKE 'KEC%'
AND EXISTS (SELECT 1
                    FROM INF_BATCH_LOG INF
                    WHERE APP.APPLICATION_RECORD_ID = INF.APPLICATION_RECORD_ID AND  INF.INTERFACE_CODE = 'CL001'
                    AND TRUNC(INF.INTERFACE_DATE) = (SELECT TRUNC(APP_DATE) FROM APPLICATION_DATE)                    )
ORDER BY AG.APPLICATION_GROUP_NO]]>
	</queryString>
