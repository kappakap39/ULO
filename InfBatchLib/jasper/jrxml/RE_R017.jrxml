	<parameter name="P_APP_DATE" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
AG.APPLICATION_GROUP_NO,
PL.DISPLAY_NAME AS PERSONAL_TYPE_DESC,
P.TH_FIRST_NAME,
P.TH_LAST_NAME,
TO_CHAR(APP.FINAL_APP_DECISION_DATE,'DD/MM/YYYY') AS FINAL_APP_DECISION_DATE,
OL.DISPLAY_NAME AS OCCUPATION_DESC,
BL.DISPLAY_NAME AS BUSINESS_TYPE_DESC,
CASE WHEN B.ORG_ID = 'KPL' THEN PT.DISPLAY_NAME ELSE CA.DISPLAY_NAME || CASE WHEN B.ORG_ID = 'CC' THEN  ' / '|| CL.DISPLAY_NAME ELSE '' END END AS CARD_LEVEL_DESC,
AG.APPLICATION_TYPE,
APP.RECOMMEND_DECISION,
XS.REASON_DESC
 FROM ORIG_APPLICATION_GROUP AG
JOIN ORIG_APPLICATION APP ON AG.APPLICATION_GROUP_ID = APP.APPLICATION_GROUP_ID
JOIN ORIG_LOAN L ON APP.APPLICATION_RECORD_ID = L.APPLICATION_RECORD_ID
LEFT JOIN ORIG_CARD C ON L.LOAN_ID = C.LOAN_ID
LEFT JOIN CARD_TYPE CT ON CT.CARD_TYPE_ID = C.CARD_TYPE
JOIN ORIG_PERSONAL_RELATION PR ON PR.REF_ID = APP.APPLICATION_RECORD_ID
JOIN ORIG_PERSONAL_INFO P ON P.PERSONAL_ID = PR.PERSONAL_ID
JOIN ORIG_KYC K ON K.PERSONAL_ID = P.PERSONAL_ID
AND (TO_NUMBER(K.CUSTOMER_RISK_GRADE)  >= 3 OR K.RELATION_FLAG = 'Y')
JOIN ORIG_APPLICATION_LOG AL ON AL.APPLICATION_GROUP_ID = APP.APPLICATION_GROUP_ID AND AL.JOB_STATE IN ('NM9901', 'NM1201')
AND  TRUNC(AL.CREATE_DATE) = (select TRUNC(app_date) from APPLICATION_DATE)
AND APP.LIFE_CYCLE = (SELECT MAX(LIFE_CYCLE) FROM ORIG_APPLICATION WHERE ORIG_APPLICATION.APPLICATION_GROUP_ID = AG.APPLICATION_GROUP_ID )
JOIN BUSINESS_CLASS B ON B.BUS_CLASS_ID = APP.BUSINESS_CLASS_ID
LEFT JOIN (
        SELECT XV.REF_ID,X.VER_RESULT_ID, LISTAGG(XL.DISPLAY_NAME,', ') WITHIN GROUP(ORDER BY XL.DISPLAY_NAME) AS REASON_DESC
        FROM  ORIG_APP.XRULES_POLICY_RULES X
        LEFT JOIN XRULES_VERIFICATION_RESULT XV ON X.VER_RESULT_ID = XV.VER_RESULT_ID
        LEFT JOIN LIST_BOX_MASTER XL ON XL.CHOICE_NO = X.REASON AND XL.FIELD_ID = '57'
        GROUP BY X.VER_RESULT_ID,XV.REF_ID
)XS ON XS.REF_ID = APP.APPLICATION_RECORD_ID
 LEFT JOIN LIST_BOX_MASTER OL ON OL.CHOICE_NO = P.OCCUPATION AND OL.FIELD_ID = '16'
 LEFT JOIN LIST_BOX_MASTER BL ON BL.CHOICE_NO = P.BUSINESS_TYPE AND BL.FIELD_ID = '21'
LEFT JOIN LIST_BOX_MASTER CL ON CL.CHOICE_NO = CT.CARD_LEVEL AND CL.FIELD_ID = '45'
LEFT JOIN LIST_BOX_MASTER PL ON PL.CHOICE_NO = P.PERSONAL_TYPE AND PL.FIELD_ID = '29'
LEFT JOIN LIST_BOX_MASTER CA ON CA.CHOICE_NO = CT.CARD_CODE AND CA.FIELD_ID = '108'
LEFT JOIN LIST_BOX_MASTER PT ON PT.SYSTEM_ID2 = APP.BUSINESS_CLASS_ID AND PT.FIELD_ID = '53'
WHERE APP.FINAL_APP_DECISION IN ('Approve')
ORDER BY AG.APPLICATION_GROUP_NO]]>
	</queryString>
