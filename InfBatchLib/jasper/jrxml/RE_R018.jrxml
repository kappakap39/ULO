	<field name="PERSONAL_TYPE_DESC" class="java.lang.String"/>
	<field name="CARDLINK_CUST_NO" class="java.lang.String"/>
	<field name="CARD_NO_MARK" class="java.lang.String"/>
	<field name="LOAN_AMT" class="java.math.BigDecimal"/>
	<field name="APP_DECISION_DATE" class="java.lang.String"/>
