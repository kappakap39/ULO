	<parameter name="P_APP_DATE" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
        AG.APPLICATION_GROUP_NO,
        LP.DISPLAY_NAME AS PERSONAL_TYPE_DESC,
        CUST.CARDLINK_CUST_NO,
        C.CARD_NO_MARK,
        L.LOAN_AMT,
        TO_CHAR(APP.FINAL_APP_DECISION_DATE,'DD/MM/YYYY') AS APP_DECISION_DATE
FROM ORIG_APPLICATION_GROUP AG
INNER JOIN ORIG_APPLICATION APP
ON AG.APPLICATION_GROUP_ID = APP.APPLICATION_GROUP_ID
INNER JOIN INF_BATCH_LOG INF ON APP.APPLICATION_RECORD_ID = INF.APPLICATION_RECORD_ID AND  INF.INTERFACE_CODE = 'CL001'
INNER JOIN ORIG_LOAN L ON APP.APPLICATION_RECORD_ID = L.APPLICATION_RECORD_ID
INNER JOIN ORIG_CARD C ON C.LOAN_ID = L.LOAN_ID
INNER JOIN ORIG_PERSONAL_RELATION PR ON PR.REF_ID = APP.APPLICATION_RECORD_ID
INNER JOIN ORIG_PERSONAL_INFO P ON PR.PERSONAL_ID = P.PERSONAL_ID
INNER JOIN ORIG_CARDLINK_CUSTOMER CUST ON C.CARDLINK_CUST_ID = CUST.CARDLINK_CUST_ID
JOIN LIST_BOX_MASTER LP ON LP.CHOICE_NO = P.PERSONAL_TYPE AND LP.FIELD_ID = '29'
WHERE AG.APPLICATION_TYPE = 'INC'
AND TRUNC(INF.INTERFACE_DATE) = (SELECT TRUNC(APP_DATE) FROM APPLICATION_DATE)
AND AG.LAST_DECISION = 'AP' AND  APP.CARDLINK_FLAG = 'S'
ORDER BY AG.APPLICATION_GROUP_NO, LP.DISPLAY_NAME, C.CARD_NO_MARK]]>
	</queryString>
