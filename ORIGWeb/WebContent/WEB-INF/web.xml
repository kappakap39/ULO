<?xml version="1.0" encoding="UTF-8"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" version="3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>ORIGWeb</display-name>
	<servlet>
		<description>
		</description>
		<display-name>SessionInspectServlet</display-name>
		<servlet-name>SessionInspectServlet</servlet-name>
		<servlet-class>com.ibm.ws.webcontainer.httpsession.SessionInspectServlet</servlet-class>
		<enabled>true</enabled>
		<async-supported>false</async-supported>
	</servlet>
	<servlet-mapping>
		<servlet-name>SessionInspectServlet</servlet-name>
		<url-pattern>/SessionInspectServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description>
		</description>
		<display-name>Ajax</display-name>
		<servlet-name>Ajax</servlet-name>
		<servlet-class>com.eaf.j2ee.pattern.control.Ajax</servlet-class>
		<enabled>true</enabled>
		<async-supported>false</async-supported>
	</servlet>
	<servlet-mapping>
		<servlet-name>Ajax</servlet-name>
		<url-pattern>/Ajax</url-pattern>
	</servlet-mapping>
	<servlet>
	  <servlet-name>rest</servlet-name>
	  <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	  <load-on-startup>2</load-on-startup>
	</servlet>
	<servlet-mapping>
	  <servlet-name>rest</servlet-name>
	  <url-pattern>/rest/controller/*</url-pattern>
	</servlet-mapping>
	<jsp-config>
		<taglib>
			<taglib-uri>/JStartTagLib</taglib-uri>
			<taglib-location>/WEB-INF/lib/JStartTagLib.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/anti_csrf</taglib-uri>
			<taglib-location>/WEB-INF/lib/anti_csrf.tld</taglib-location>
		</taglib>
	</jsp-config>
	
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>UsersCollection</web-resource-name>
			<url-pattern>/FrontController</url-pattern>
			<url-pattern>/DownloadServlet</url-pattern>
			<url-pattern>/ViewReportServlet</url-pattern>
			<url-pattern>/Ajax</url-pattern>
			<url-pattern>/UploadServlet</url-pattern>
			<url-pattern>/ViewImage</url-pattern>
			<http-method>GET</http-method>
			<http-method>PUT</http-method>
			<http-method>HEAD</http-method>
			<http-method>TRACE</http-method>
			<http-method>POST</http-method>
			<http-method>DELETE</http-method>
			<http-method>OPTIONS</http-method>
		</web-resource-collection>
		<auth-constraint>
			<role-name>AllUserLogin</role-name>
		</auth-constraint>
	</security-constraint>
	
	<!-- 
	<login-config>
		<auth-method>FORM</auth-method>
		<form-login-config>
			<form-login-page>/login.jsp</form-login-page>
			<form-error-page>/logon_error.jsp</form-error-page>
		</form-login-config>
	</login-config>
	 -->
	<security-role>
		<description>
		</description>
		<role-name>AllUserLogin</role-name>
	</security-role>
	<resource-ref>
		<description/>
		<res-ref-name>jdbc/orig</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
		<res-sharing-scope>Shareable</res-sharing-scope>
	</resource-ref>
	<resource-ref>
		<description/>
		<res-ref-name>jdbc/origwf</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
		<res-sharing-scope>Shareable</res-sharing-scope>
	</resource-ref>
	<resource-ref>
		<description/>
		<res-ref-name>jdbc/dih</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
		<res-sharing-scope>Shareable</res-sharing-scope>
	</resource-ref>
	<resource-ref>
		<description/>
		<res-ref-name>jdbc/bpm</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
		<res-sharing-scope>Shareable</res-sharing-scope>
	</resource-ref>
	<resource-ref>
		<description />

		<res-ref-name>jdbc/ias</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
		<res-sharing-scope>Shareable</res-sharing-scope>
	</resource-ref>
	

</web-app>